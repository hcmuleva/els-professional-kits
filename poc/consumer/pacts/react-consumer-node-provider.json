{
  "consumer": {
    "name": "react-consumer"
  },
  "interactions": [
    {
      "description": "a request for user with ID 1",
      "providerState": "user with ID 1 exists",
      "request": {
        "method": "GET",
        "path": "/api/users/1"
      },
      "response": {
        "body": {
          "email": "harish@example.com",
          "id": 1,
          "username": "Harish"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.email": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.username": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "node-provider"
  }
}